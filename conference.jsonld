    },
	{
      "id": "https://w3id.org/xapi/conference/templates#attended",
      "inScheme": "https://w3id.org/xapi/conference/v1.0.3",
      "prefLabel": {
        "en": "Attended"
      },
      "definition": {
        "en": "The statement template associated with the actor click on the presentaiton link."
      },
      "type":"StatementTemplate",
      "verb": "http://adlnet.gov/expapi/verbs/attended",
      "rules": [
       
      ]
    },
	{
      "id": "https://w3id.org/xapi/conference/templates#commented",
      "inScheme": "https://w3id.org/xapi/conference/v1.0.3",
      "prefLabel": {
        "en": "Commented"
      },
      "definition": {
        "en": "The statement template associated with the actor commented any content - click on send comment button. "
      },
      "type":"StatementTemplate",
      "verb": "http://adlnet.gov/expapi/verbs/commented",
      "rules": [
                {
                    "location": "result.response",
                    "presence": "included"
                }
            ]
    },
	{
      "id": "https://w3id.org/xapi/conference/templates#rated",
      "inScheme": "https://w3id.org/xapi/conference/v1.0.3",
      "prefLabel": {
        "en": "Rated"
      },
      "definition": {
        "en": "The statement template associated with the actor rate presentation - clike on the rate link. "
      },
      "type":"StatementTemplate",
      "verb": "http://id.tincanapi.com/verb/rated",
      "rules": [
                {
                    "location": "result.response",
                    "presence": "included"
                }
            ]
    },
	{
      "id": "https://w3id.org/xapi/conference/templates#terminated",
      "inScheme": "https://w3id.org/xapi/conference",
      "prefLabel": {
        "en": "Terminated"
      },
      "definition": {
        "en": "The statement template and rules associated with a terminating the session on the website."
      },
      "type":"StatementTemplate",
      "verb": "http://adlnet.gov/expapi/verbs/terminated",
      "objectActivityType": "https://w3id.org/xapi/conference/activity-type/page",
      "rules": [
        
      ]
    }
    
    
  ],
  "patterns": [
      {
         "id": "https://w3id.org/xapi/conference/patterns#generalpattern",
         "inScheme": "https://w3id.org/xapi/conference",
         "type":"Pattern",
         "primary": true,
         "prefLabel": {
           "en": "General Pattern"
         },
          "definition": {
              "en": "The general pattern and sequence of Statement templates using the Conference Profile."
          },
          "sequence": ["https://w3id.org/xapi/conference/templates#initialized", "https://w3id.org/xapi/conference/patterns#optionalmiddlestatements", "https://w3id.org/xapi/conference/templates#terminated"]
      },
      {
          "id": "https://w3id.org/xapi/conference/patterns#all-activities-pattern",
          "inScheme": "https://w3id.org/xapi/conference/v1.0.3",
          "type":"Pattern",
          "prefLabel": {
            "en": "All Activities Pattern"
          },
           "definition": {
               "en": "All of the Conference Profile statement templates."
           },
          "alternates": ["https://w3id.org/xapi/conference/templates#launched", "https://w3id.org/xapi/conference/templates#attended", "https://w3id.org/xapi/conference/templates#commented",
          "https://w3id.org/xapi/conference/templates#rated",
		  "https://w3id.org/xapi/video/templates#played", "https://w3id.org/xapi/video/templates#paused", "https://w3id.org/xapi/video/templates#seeked", "https://w3id.org/xapi/video/templates#volumechange", "https://w3id.org/xapi/video/templates#screenchange", "https://w3id.org/xapi/video/templates#closed-captioning", "https://w3id.org/xapi/video/templates#completed"]
      },
      {
          "id": "https://w3id.org/xapi/conference/patterns#optionalmiddlestatements",
          "inScheme": "https://w3id.org/xapi/conference",
          "type":"Pattern",
          "prefLabel": {
            "en": "Optional Middle Statements"
          },
           "definition": {
               "en": "A combined pattern of zero or more Conference Profile standard statement templates that can be used with the general pattern."
           },
     
			"zeroOrMore": "https://w3id.org/xapi/conference/patterns#all-activities-pattern"

      }

  ]
}
