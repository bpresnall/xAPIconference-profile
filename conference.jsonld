{
  "@context": "https://w3id.org/xapi/profiles/context",
  "id": "https://w3id.org/xapi/conference",
  "type": "Profile",
  "conformsTo": "https://w3id.org/xapi/profiles#1.0",
  "prefLabel": {
    "en": "Conference Profile"
  },
  "definition": {
    "en": "The conference profile of the xAPI was created to identify and standardize the common types of interactions that can be tracked in conference websites."
  },
  "author": {
    "type": "Organization",
    "name": "Jefferson Institute - xAPI conference profile"
  },
  "concepts": [
    {
      "id": "https://w3id.org/xapi/conference/verbs/initialized",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
      "exactMatch": [  "http://id.tincanapi.com/verb/initialized "],
      "definition": {
        "en": "Indicates the actor logged in to the conference website."
      },
      "prefLabel": {
        "en": "initialized"
      }
    },
    {
      "id": "https://w3id.org/xapi/conference/verbs/launched",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
      "exactMatch": [ "http://adlnet.gov/expapi/verbs/launched" ],
      "definition": {
        "en": "Indicates that the actor open page."
      },
      "prefLabel": {
        "en": "launched"
      }
    },
    {
      "id": "https://w3id.org/xapi/conference/verbs/attended ",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	  "exactMatch": [ "http://adlnet.gov/expapi/verbs/attended" ],
      "definition": {
        "en": "Indicates the actor click on presentation link"
      },
      "prefLabel": {
        "en": "attended"
      }
    },
    {
      "id": "https://w3id.org/xapi/conference/activity-type/commented",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	  "exactMatch": [ "http://adlnet.gov/expapi/verbs/commented" ],
      "definition": {
        "en": "Indicate the actor commented any content - click on send comment button. Result reponse is comment text"
      },
      "prefLabel": {
        "en": "commented"
      }
    }, 
	{
      "id": "https://w3id.org/xapi/video/activity-type/played",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	  "exactMatch": [ "https://w3id.org/xapi/video/verbs/played" ],
      "definition": {
        "en": "Used when the actor played a video or clicked the play button. "
      },
      "prefLabel": {
        "en": "played"
      }
    },
	{
      "id": "https://w3id.org/xapi/video/activity-type/paused",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	  "exactMatch": [ "https://w3id.org/xapi/video/verbs/paused" ],
      "definition": {
        "en": "Indicates that the actor temporary or permanently stopped experiencing the recorded media object "
      },
      "prefLabel": {
        "en": "paused"
      }
    },
	{
      "id": "https://w3id.org/xapi/video/activity-type/seeked",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	  "exactMatch": [ "https://w3id.org/xapi/video/verbs/seeked" ],
      "definition": {
        "en": "Used in combination with time-from and time-to extensions when the Actor moves the progress bar forward or backward to a specific time in the video."
      },
      "prefLabel": {
        "en": "seeked"
      }
    },
	{
      "id": "https://w3id.org/xapi/video/activity-type/interacted",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	   "exactMatch": [ "http://adlnet.gov/expapi/verbs/interacted" ],
      "definition": {
        "en": "Used to express that the actor interacted with the player (except play, pause, seek). e.g. mute, unmute, change resolution, change player size"
      },
      "prefLabel": {
        "en": "interacted"
      }
    },
	{
      "id": "https://w3id.org/xapi/video/activity-type/completed",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	  "exactMatch": [ "http://adlnet.gov/expapi/verbs/completed" ],
      "definition": {
        "en": "Used to express that the actor completed a video by watching all parts of the video at least once."
      },
      "prefLabel": {
        "en": "completed"
      }
    },
	{
      "id": "https://w3id.org/xapi/conference/activity-type/terminated",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "Verb",
	   "exactMatch": [ "http://adlnet.gov/expapi/verbs/terminated" ],
      "definition": {
        "en": "Used to express that the actor completed a video by watching all parts of the video at least once or terminated session on website (logged out)."
      },
      "prefLabel": {
        "en": "terminated"
      }
    },
	{
      "id": "https://w3id.org/xapi/conference/activity-type/rated",
      "inScheme": "https://w3id.org/xapi/rated",
      "type": "Verb",
	   "exactMatch": [ "http://id.tincanapi.com/verb/rated" ],
      "definition": {
        "en": "Used to rate presentation, result is in range (1,5)."
      },
      "prefLabel": {
        "en": "terminated"
      }
    },
	
	{
      "id": "https://w3id.org/xapi/conference/activity-type/page",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "ActivityType",
      "definition": {
        "en": "A webpage which includes presentations or discussions."
      },
      "prefLabel": {
        "en": "page"
      }
    },
	{
      "id": "https://w3id.org/xapi/conference/activity-type/presentation",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "ActivityType",
      "definition": {
        "en": "A webpage with presentation."
      },
      "prefLabel": {
        "en": "page"
      }
    },
	{
      "id": "https://w3id.org/xapi/conference/activity-type/video",
      "inScheme": "https://w3id.org/xapi/conference",
      "type": "ActivityType",
      "definition": {
        "en": "A recording of both the visual and audible components made available on a display screen."
      },
      "prefLabel": {
        "en": "video"
      }
    }
	
    
  ],
  
  "templates": [
    {
      "id": "https://w3id.org/xapi/conference/templates#initialized",
      "inScheme": "https://w3id.org/xapi/conference/v1.0.3",
      "prefLabel": {
        "en": "Initialized"
      },
      "definition": {
        "en": "The statement template associated with the actor logged in to the conference website."
      },
      "type":"StatementTemplate",
      "verb": "http://id.tincanapi.com/verb/initialized",
      "rules": [
       
