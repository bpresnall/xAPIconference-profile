    },
	{
      "id": "https://w3id.org/xapi/conference/templates#launched",
      "inScheme": "https://w3id.org/xapi/conference/v1.0.3",
      "prefLabel": {
        "en": "Launched"
      },
      "definition": {
        "en": "The statement template associated with the actor launch webpage with presentaiton."
      },
      "type":"StatementTemplate",
      "verb": "http://adlnet.gov/expapi/verbs/launched",
      "rules": [
       
